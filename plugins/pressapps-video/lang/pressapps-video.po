#: includes/help.php:14
msgid "<h2>Video Shortcode</h2>"
msgstr ""

#: includes/help.php:16
msgid ""
"You can use <code>[pa_video]</code> shortcode to include the Videos on any "
"page, post or custom post type."
msgstr ""

#: includes/help.php:18
msgid "The shortcode accepts four optional attributes:"
msgstr ""

#: includes/help.php:19
msgid "(1) <b>category</b> = <i>-1</i> <b>|</b> <i>{any video category id}</i>"
msgstr ""

#: includes/help.php:20
msgid "(2) <b>featured</b> = <i>75</i> <b>|</b> <i>{any video post id}</i>"
msgstr ""

#: includes/help.php:21
msgid "(3) <b>template</b> = <i>search</i>"
msgstr ""

#: includes/help.php:22
msgid "(4) <b>lightbox</b> = <i>true</i>"
msgstr ""

#: includes/help.php:23
msgid "<b>Examples</b>"
msgstr ""

#: includes/help.php:24
msgid "1. <code>[pa_video]</code>"
msgstr ""

#: includes/help.php:25
#, php-format
msgid ""
"2. <code>[pa_video category={category_id}]</code> {category_id} you will "
"find it <a href=\"%s\">here</a> under shortcode column"
msgstr ""

#: includes/help.php:26
msgid ""
"3. <code>[pa_video category={category_id} featured={video_post_id} template="
"\"search\" lightbox=\"true\"]</code>"
msgstr ""

